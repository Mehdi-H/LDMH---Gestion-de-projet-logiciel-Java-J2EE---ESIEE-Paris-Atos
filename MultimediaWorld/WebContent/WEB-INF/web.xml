<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <include-prelude>/WEB-INF/_taglibs.jsp</include-prelude>
    </jsp-property-group>
  </jsp-config>
  <servlet>
    <servlet-name>Presentation</servlet-name>
    <servlet-class>servlets.Presentation</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Rubrique</servlet-name>
    <servlet-class>servlets.Rubrique</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Recherche</servlet-name>
    <servlet-class>servlets.Recherche</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ModifierPanier</servlet-name>
    <servlet-class>servlets.ModifierPanier</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Connexion</servlet-name>
    <servlet-class>servlets.Connexion</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Deconnexion</servlet-name>
    <servlet-class>servlets.Deconnexion</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Panier</servlet-name>
    <servlet-class>servlets.Panier</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Commande</servlet-name>
    <servlet-class>servlets.Commande</servlet-class>
  </servlet>
  <welcome-file-list>
    <welcome-file>Presentation</welcome-file>
  </welcome-file-list>
  <servlet-mapping>
    <servlet-name>Presentation</servlet-name>
    <url-pattern>/presentation</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Rubrique</servlet-name>
    <url-pattern>/rubriques/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Recherche</servlet-name>
    <url-pattern>/recherche</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ModifierPanier</servlet-name>
    <url-pattern>/modifier_panier</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Connexion</servlet-name>
    <url-pattern>/connexion</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Deconnexion</servlet-name>
    <url-pattern>/deconnexion</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Panier</servlet-name>
    <url-pattern>/panier</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Commande</servlet-name>
    <url-pattern>/commande</url-pattern>
  </servlet-mapping>
</web-app>